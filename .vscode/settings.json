{
    "editor.mouseWheelZoom": true,
    "editor.minimap.showSlider": "always",
    "editor.fontSize": 15,
    "cmake.buildArgs": [
        "-v"
    ],
    "$comment1": "Should be something else than -v for unix/linux makefiles",
    "cmake.buildToolArgs": [
        "-v"
    ],
    "cmake.sourceDirectory": "${workspaceFolder}",
    "cmake.configureOnOpen": true,
    "cmake.options.statusBarVisibility": "visible",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "files.associations": {
        "cctype": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "bitset": "cpp",
        "charconv": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdint": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "optional": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "span": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp",
        "variant": "cpp",
        "format": "cpp",
        "__bit_reference": "cpp",
        "__locale": "cpp",
        "__split_buffer": "cpp",
        "__verbose_abort": "cpp",
        "ios": "cpp",
        "locale": "cpp",
        "print": "cpp",
        "clocale": "cpp",
        "cwchar": "cpp",
        "chrono": "cpp",
        "condition_variable": "cpp",
        "forward_list": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "ratio": "cpp",
        "fstream": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "semaphore": "cpp",
        "sstream": "cpp",
        "stop_token": "cpp",
        "thread": "cpp",
        "__threading_support": "cpp",
        "codecvt": "cpp",
        "cwctype": "cpp",
        "map": "cpp",
        "iomanip": "cpp"
    },
    "C_Cpp.codeFolding": "disabled",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c23",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.parameterNames.suppressWhenArgumentContainsName": true,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.clang_format_style": "Google",
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.intelliSenseMemoryLimit": 16384,
    "C_Cpp.files.exclude": {
        "**/.vscode": true,
        "**/.vs": true,
        "**/build": true,
        "**/Debug": true,
        "**/Debug_Test": true,
        "**/Unit_Test": true,
        "**/Release": true,
        "/usr/include/boost": true,
        "/usr/include/python3": true,
        "/usr/include/qt4": true,
    },
    "cmake.configureArgs": [
        "--fresh"
    ]
}
